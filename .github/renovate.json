{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "prHourlyLimit": 3,
  "argocd": {
    "fileMatch": [
      "funland/[^/]+/application\\.yaml$"
    ]
  },
  "kubernetes": {
    "fileMatch": [
      "manifests/[^/]+/deployment\\.yaml$"
    ]
  }
}